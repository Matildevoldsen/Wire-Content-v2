{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "awcodes/filament-curator": "^3.6",
        "awcodes/filament-tiptap-editor": "^3.0",
        "bezhansalleh/filament-shield": "^3.2",
        "codewithdennis/filament-price-filter": "^0.0.4",
        "cwsps154/app-settings": "^1.0",
        "cyrildewit/eloquent-viewable": "^7.0",
        "datlechin/filament-menu-builder": "^0.5.3",
        "dotswan/filament-laravel-pulse": "^1.1",
        "filament/filament": "^v3.2.115",
        "hasnayeen/themes": "^3.0",
        "http-interop/http-factory-guzzle": "^1.2",
        "laravel/cashier": "^15.4",
        "laravel/framework": "^v12.9.2",
        "laravel/jetstream": "^v5.3.6",
        "laravel/pulse": "^v1.4.1",
        "laravel/sanctum": "^v4.0.8",
        "laravel/scout": "^v10.14.1",
        "laravel/socialite": "^v5.15.1",
        "laravel/tinker": "^v2.10.1",
        "livewire/livewire": "^v3.5.6",
        "lukepolo/laracart": "^2.5.0",
        "marcogermani87/filament-captcha": "^1.4",
        "matildevoldsen/wire-comments": "v0.6.7",
        "matildevoldsen/wire-content": "@dev",
        "meilisearch/meilisearch-php": "^1.11",
        "owenvoke/blade-fontawesome": "^2.6",
        "pxlrbt/filament-spotlight": "^1.2",
        "ralphjsmit/laravel-filament-seo": "^1.3",
        "robsontenorio/mary": "*",
        "saade/filament-adjacency-list": "^3.2",
        "spatie/laravel-markdown": "2.7.*",
        "spatie/shiki-php": "^2.3",
        "visualbuilder/email-templates": "^3.1.10",
        "z3d0x/filament-fabricator": "^2.2"
    },

    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.20",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^v3.8.2",
        "pestphp/pest-plugin-laravel": "^v3.1.0"
    },

    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/wire-content",
            "options": { "symlink": true }
        }
    ],
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\\\Foundation\\\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
